{
    "presets": [
        "env",
        "react",
        "stage-2"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "react-hot-loader/babel"
    ],
    "env": {
        "production": {
            "plugins": [
                "transform-react-remove-prop-types"
            ]
        },
        "test": {
            "plugins": [
                "transform-decorators-legacy",
                "transform-object-rest-spread",
                "istanbul"
            ]
        }
    }
}
